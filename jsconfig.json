{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@css/*": ["assets/css/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@/*": ["pages/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", "out", ".next"]
}
